<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.companymodelsupdate.Models"
		table="models">
		<meta attribute="class-attribute">
			these class contains models details.
		</meta>

		<id name="mId" type="int" column="mid" />

		<property name="mName" column="mname" type="string" />
		<property name="fKCompanyId" column="FKcompany_id" insert="false" />
	</class>
</hibernate-mapping>