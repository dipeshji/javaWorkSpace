
class ConvertToString{
	private int a;
	private float b;
	
	ConvertToString(int a,float b){
		this.a = a;
		this.b = b;
	}
	
	public float add() {
		return a+b;
	}
}

public class ValueOf {
public static void main(String args[]) {
	
//	===================================================
	int value = 10;
	String intToString = String.valueOf(value);
	System.out.println(intToString+20);
	
	
//	=====================================================
	boolean bool = true;
	boolean bool1 = false;
	
	String boolToString = String.valueOf(bool);
	String bool1ToString = String.valueOf(bool1);
	
	System.out.println(boolToString+"=> String");
	System.out.println(bool1ToString+"=> String");
	
	
//	====================================================
	char ch = 'A';
	char ch2 = 'B';
	
	String charToString = String.valueOf(ch);
	String char2ToString = String.valueOf(ch2);
	
	System.out.println(charToString);
	System.out.println(char2ToString);
	
//	======================================================
	float f = 20.05f;
	double d = 10.02;
	
	String floatToString = String.valueOf(f);
	String doubleToString = String.valueOf(d);
	
	System.out.println(floatToString);
	System.out.println(doubleToString);
	
//	=======================================================
	ConvertToString convertToString = new ConvertToString(10,20.05f);
	System.out.println("Add Result => "+convertToString.add());
	String toStringObject = convertToString.toString();
	String objectToString = String.valueOf(toStringObject);
	System.out.println(objectToString);
	
}
}
